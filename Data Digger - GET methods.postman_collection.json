					"} else if (environmentName === 'acc') {",
					"    url = 'http://DLSYLLD71AD01.lhsystems.int:7000/lido/las/authService/authenticate';",
					"}",
					"",
					"const credentials = {",
					"    userId: 'lidoapi', ",
					"    password: 'p2reK%234t3sV%26N!pN',",
					"    domain: 'LIDO',",
					"}",
					"",
					"const getTokenRequest = {",
					"  method: 'POST',",
					"  url: url,",
					"  body: {",
					"      mode: 'formdata',",
					"      formdata: [",
					"          { key: 'userId', value: credentials.userId },",
					"          { key: 'password', value: credentials.password },",
					"          { key: 'domain', value: credentials.domain }",
					"      ]",
					"  }",
					"};",
					"",
					"pm.sendRequest(getTokenRequest, (err, response) => {",
					"  const jsonResponse = response.json();",
					"  const newAccessToken = jsonResponse[\"token\"];",
					"  pm.environment.set('token', newAccessToken);",
					"});"
